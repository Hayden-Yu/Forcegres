    "@types/moment-timezone": "^0.5.12",
    "@types/pg": "^7.4.14",
    "@types/winston": "^2.4.4",
    "jsforce": "^1.9.2",
    "moment-timezone": "^0.5.26",
    "pg": "^7.11.0",
    "typescript": "^3.5.3",
