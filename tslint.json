{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
    },
    "rules": {
        "arrow-parens": false,
        "array-type": false,
        "no-string-literal": false,
        "quotemark": {
            "options": [
                "single"
            ]
        },
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "object-literal-key-quotes": false,
        "variable-name": {
            "options": [
                "allow-leading-underscore"
            ]
        },
        "max-classes-per-file": false,
        "member-ordering": false
    },
    "rulesDirectory": []
}